            <xml-property name="queryText"><![CDATA[SELECT 
	CLASSNAME,
	STUDENTNAME,
	STUDENT_NO,
	IC_NO,
	CONVERT(ISNOTALLOW, UNSIGNED INTEGER) REMARKS
FROM (
	SELECT 
		CRD.NAME AS CLASSNAME,
		S.NAME AS STUDENTNAME, 
		S.STUDENT_NO,
		S.IC_NO,
		SUM(IF(DAI.DAILY_RECORD_DISCIPLINE IS NOT NULL, 1, 0)) ISNOTALLOW
	FROM 
		STUDENT S 
		INNER JOIN RECORDS R ON S.PKID = R.STUDENT 
		INNER JOIN CLASS_ROOM_DETAIL CRD ON R.CLASS_ROOM_DETAIL = CRD.PKID 
		INNER JOIN DAILY_ACTIVITY DA ON R.PKID = DA.RECORDS 
		INNER JOIN DAILY_ACTIVITY_ITEM DAI ON DAI.DAILY_ACTIVITY = DA.PKID 
	WHERE
		MONTH(DA.DATE) = ? AND YEAR(DA.DATE) = ? 
		AND R.STATUS_FLAG = TRUE 
	GROUP BY CRD.NAME, S.NAME 
	ORDER BY CRD.NAME, S.NAME 
) S INNER JOIN (
	SELECT CONVERT(VALUE1, UNSIGNED INTEGER) MAXDAY FROM MISC_ENTITY WHERE CATEGORY = 'MAX_DISCIPLINE_ALLOWED_TO_OUTING'
) MD WHERE S.ISNOTALLOW < MD.MAXDAY;]]></xml-property>
