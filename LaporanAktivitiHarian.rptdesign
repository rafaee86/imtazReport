            <xml-property name="queryText"><![CDATA[SELECT 
	S.STUDENT_NO AS STUDENTNO,
	S.NAME AS NAME, 
	S.IC_NO AS ICNO,
	CONCAT(CRD.CLASS_ROOM,' ',CRD.NAME) CLASSROOM, 
	CONCAT(T.SALUTATION,' ',T.NAME) TEACHER  
FROM 
	STUDENT S 
	INNER JOIN RECORDS R ON R.STUDENT = S.PKID 
	INNER JOIN CLASS_ROOM_DETAIL CRD ON R.CLASS_ROOM_DETAIL = CRD.PKID 
	INNER JOIN TEACHER T ON T.PKID = CRD.TEACHER  
WHERE 
	R.STATUS_FLAG = TRUE 
	AND S.STUDENT_NO = ?]]></xml-property>
