            <xml-property name="queryText"><![CDATA[SELECT 
	DATE(DA.DATE) AS TRANSDATE,
	DRI.DESCRIPTION AS ITEMDESC,
	CONCAT('(',DRD.CODE,') ',DRD.DESCRIPTION) AS DISCIPLINEDESC 
FROM 
	STUDENT S 
	INNER JOIN RECORDS R ON S.PKID = R.STUDENT  
	INNER JOIN DAILY_ACTIVITY DA ON R.PKID = DA.RECORDS 
	INNER JOIN DAILY_ACTIVITY_ITEM DAI ON DAI.DAILY_ACTIVITY = DA.PKID 
	INNER JOIN DAILY_RECORD_ITEM DRI ON DRI.PKID = DAI.DAILY_RECORD_ITEM 
	LEFT JOIN DAILY_RECORD_DISCIPLINE DRD ON DRD.PKID = DAI.DAILY_RECORD_DISCIPLINE 
WHERE 
	MONTH(DA.DATE) = ? AND YEAR(DA.DATE) = ? 
	AND R.STATUS_FLAG = TRUE AND UPPER(S.STUDENT_NO) = UPPER(?) 
ORDER BY DA.DATE, DRI.SEQUENCE]]></xml-property>
