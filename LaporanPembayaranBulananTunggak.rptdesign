            <xml-property name="queryText"><![CDATA[SELECT 
	STUDENT_NO,
	HPNO,
	NAME,
	PAYAMAOUNT,
	TOTALAMN,
	IF(TOTALAMN - PAYAMAOUNT >= 0, TOTALAMN - PAYAMAOUNT,0) BALANCEAMN 
FROM 
(
	SELECT 
		S.STUDENT_NO,
		S.PARENT_HP_NO HPNO,
		S.NAME,
		IFNULL((
			SELECT 
				SUM(PII.AMOUNT) SUMAMN
			FROM 
				PAYMENT P 
				INNER JOIN PAYMENT_ITEM PII ON PII.PAYMENT = P.PKID 
				INNER JOIN PAYMENT_DESCRIPTION PD ON PD.PKID = PII.PAYMENT_DESCRIPTION 
				INNER JOIN PAYMENT_MONTH PM ON PM.PAYMENT = P.PKID 
			WHERE 
				P.RECORDS = R.PKID 
				AND P.STATUS_FLAG = TRUE 
				AND P.YEAR = ?
				AND PM.MONTH = ? 
		),0) PAYAMAOUNT,  
		PDT.TOTALAMN 
	FROM 
		STUDENT S 
		INNER JOIN RECORDS R ON S.PKID = R.STUDENT 
		INNER JOIN CLASS_ROOM_DETAIL CRD ON CRD.PKID = R.CLASS_ROOM_DETAIL 
		INNER JOIN (
			SELECT 
				SUM(AMOUNT) TOTALAMN 
			FROM 
				PAYMENT_DESCRIPTION 
			WHERE 
				STATUS_FLAG IS TRUE 
		) PDT 
	WHERE 
		R.STATUS_FLAG IS TRUE 
		AND CRD.PKID = ? 
) X 
ORDER BY X.STUDENT_NO ASC]]></xml-property>
