            <xml-property name="queryText"><![CDATA[SELECT 
	S.STUDENT_NO,
	S.IC_NO,
	S.NAME,
	P.TRANSACTION_ID,
	PD.DESCRIPTION,
	PII.AMOUNT,
	P.BANK,
	P.REFERENCE_ID,
	P.PAYMENT_TYPE 
FROM 
	STUDENT S 
	INNER JOIN RECORDS R ON S.PKID = R.STUDENT 
	INNER JOIN CLASS_ROOM_DETAIL CRD ON CRD.PKID = R.CLASS_ROOM_DETAIL 
	INNER JOIN PAYMENT P ON R.PKID = P.RECORDS 
	INNER JOIN PAYMENT_ITEM PII ON PII.PAYMENT = P.PKID 
	INNER JOIN PAYMENT_DESCRIPTION PD ON PD.PKID = PII.PAYMENT_DESCRIPTION 
	INNER JOIN PAYMENT_MONTH PM ON PM.PAYMENT = P.PKID 
WHERE 
	P.STATUS_FLAG = TRUE 
	AND P.YEAR = ?
	AND PM.MONTH = ? 
	AND CRD.PKID = ? 
ORDER BY 
	S.STUDENT_NO,
	P.TRANSACTION_ID, 
	PD.DESCRIPTION ASC]]></xml-property>
